doctype html
html
  head
    meta(charset='UTF-8')
    title TwentyOne
    meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')
    link(href='../../css/bootstrap.min.css', rel='stylesheet', type='text/css')
    link(href='../../css/font-awesome.min.css', rel='stylesheet', type='text/css')
    link(href='../../css/ionicons.min.css', rel='stylesheet', type='text/css')
    link(href='../../css/AdminLTE.css', rel='stylesheet', type='text/css')
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js')
  body.skin-twentyone.fixed
    header.header
      a.logo(href='../../index.html')
        | TwentyOne
      // Header Navbar: style can be found in header.less
      nav.navbar.navbar-static-top(role='navigation')
        a.navbar-btn.sidebar-toggle(href='#', data-toggle='offcanvas', role='button')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        .navbar-right
          ul.nav.navbar-nav
            li.dropdown.user.user-menu
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.fa.fa-plus.fa-2x
            // Messages: style can be found in dropdown.less
            li.dropdown.messages-menu
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.fa.fa-envelope
                span.label.label-success 4
              ul.dropdown-menu
                li.header You have 4 messages
                li
                  // inner menu: contains the actual data
                  ul.menu
                    li
                      // start message
                      a(href='#')
                        .pull-left
                          img.img-circle(src='../../img/avatar3.png', alt='User Image')
                        h4
                          | Support Team
                          small
                            i.fa.fa-clock-o
                            |  5 mins
                        p Why not buy a new awesome theme?
                    // end message
                    li
                      a(href='#')
                        .pull-left
                          img.img-circle(src='../../img/avatar2.png', alt='user image')
                        h4
                          | AdminLTE Design Team
                          small
                            i.fa.fa-clock-o
                            |  2 hours
                        p Why not buy a new awesome theme?
                li.footer
                  a(href='#') See All Messages
            // User Account: style can be found in dropdown.less
            li.dropdown.user.user-menu
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.glyphicon.glyphicon-user
                span
                  | Jane Doe 
                  i.caret
              ul.dropdown-menu
                // User image
                li.user-header.bg-light-blue
                  img.img-circle(src='../../img/avatar3.png', alt='User Image')
                  p
                    | Jane Doe - Web Developer
                    small Member since Nov. 2012
                // Menu Body
                li.user-body
                  .col-xs-4.text-center
                    a(href='#') Followers
                  .col-xs-4.text-center
                    a(href='#') Sales
                  .col-xs-4.text-center
                    a(href='#') Friends
                // Menu Footer
                li.user-footer
                  .pull-left
                    a.btn.btn-default.btn-flat(href='#') Profile
                  .pull-right
                    a.btn.btn-default.btn-flat(href='#') Sign out
    .wrapper.row-offcanvas.row-offcanvas-left
      // Left side column. contains the logo and sidebar
      aside.left-side.sidebar-offcanvas
        // sidebar: style can be found in sidebar.less
        section.sidebar
          // Sidebar user panel
          .user-panel
            .pull-left.image
              img.img-circle(src='../../img/avatar3.png', alt='User Image')
            .pull-left.info
              p Hello, Jane
              a(href='#')
                i.fa.fa-circle.text-success
                |  Online
          // search form
          form.sidebar-form(action='#', method='get')
            .input-group
              input.form-control(type='text', name='q', placeholder='Search...')
              span.input-group-btn
                button#search-btn.btn.btn-flat(type='submit', name='seach')
                  i.fa.fa-search
          // /.search form
          // sidebar menu: : style can be found in sidebar.less
          ul.sidebar-menu
            li.treeview.active
              a(href='#')
                i.fa.fa-picture-o
                span Artworks
                i.fa.fa-angle-left.pull-right
              ul.treeview-menu
                li.active
                  a(href='../charts/morris.html')  hottest
                li
                  a(href='../charts/flot.html')  latest
            li.treeview
              a(href='#')
                i.fa.fa-group
                span Artists
                i.fa.fa-angle-left.pull-right
              ul.treeview-menu
                li
                  a(href='../UI/general.html')  hottest
                li
                  a(href='../UI/icons.html')  famous
